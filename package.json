{
  "name": "waosNode",
  "description": "Stack JavaScript with MongoDB, ORM(sequelize ...), Express, and Node.js. (Based on meanjs and riess.js)",
  "version": "0.2.0",
  "private": false,
  "author": "https://github.com/weareopensource/Node/graphs/contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/weareopensource/Node.git"
  },
  "engines": {
    "node": ">=9.11.2",
    "npm": ">=6.4.1",
    "yarn": ">=1.13.0"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development node server.js",
    "debug": "cross-env NODE_ENV=development nodemon --inspect server.js",
    "prod": "cross-env NODE_ENV=production pm2 start server.js --name=waosNode",
    "test": "cross-env NODE_ENV=test gulp test",
    "testWatch": "cross-env NODE_ENV=test gulp testWatch",
    "testCoverage": "cross-env NODE_ENV=test gulp testCoverage",
    "testCoveralls": "cross-env NODE_ENV=test gulp testCoverage && cat ./coverage/lcov.info | coveralls",
    "generateSSLCerts": "lib/scripts/generate-ssl-certs.sh",
    "lint": "gulp lint",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect",
    "commit": "npx git-cz",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "acl": "~0.4.11",
    "bcrypt": "^3.0.4",
    "body-parser": "^1.18.3",
    "chalk": "~2.4.2",
    "compression": "^1.7.3",
    "consolidate": "^0.15.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "express": "^4.16.4",
    "express-winston": "^3.1.0",
    "generate-password": "~1.4.1",
    "glob": "~7.1.3",
    "google-auth-library": "^3.1.0",
    "helmet": "~3.16.0",
    "idtoken-verifier": "^1.2.0",
    "joi": "^14.3.1",
    "joi-zxcvbn": "^4.0.0",
    "jsonwebtoken": "^8.5.0",
    "lodash": "^4.17.11",
    "lusca": "~1.6.1",
    "method-override": "~3.0.0",
    "mongoose": "~5.4.18",
    "multer": "~1.4.1",
    "nodemailer": "~5.1.1",
    "nodemon": "^1.18.10",
    "object-path": "^0.11.4",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "~1.0.0",
    "pm2": "^3.3.1",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "sequelize": "^5.1.0",
    "supertest": "^4.0.0",
    "swig": "^1.4.2",
    "winston": "^3.2.1",
    "zxcvbn": "^4.4.2",
    "snyk": "^1.136.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "commitizen": "^3.0.7",
    "coveralls": "~3.0.3",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.15.1",
    "gulp": "~4.0.0",
    "gulp-eslint": "^5.0.0",
    "gulp-load-plugins": "^1.5.0",
    "gulp-nodemon": "~2.4.2",
    "gulp-refresh": "~1.1.0",
    "husky": "^1.3.1",
    "jest-cli": "^24.3.1",
    "standard-version": "^5.0.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "snyk": true
}
